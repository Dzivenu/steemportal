<schemalist>
    <schema id="org.ascension.steemportal" path="/org/ascension/steemportal/">

    <key name="postkey" type="s">
      <default >""</default>
      <summary>Your Steem Posting/Voting key</summary>
      <description>
        Stores your Steem Posting/Voting key
      </description>
    </key>

    <key name="activekey" type="s">
      <default >""</default>
      <summary>Your Steem Market/Transfer key</summary>
      <description>
        Stores your Steem Market/Transfer key
      </description>
    </key>

    <key name="memo" type="s">
      <default >""</default>
      <summary>Your Steem Memo key</summary>
      <description>
        Stores your Steem Memo key
      </description>
    </key>

    <key name="lastviewed" type="s">
        <default>""</default>
        <summary>Last viewed steem URL</summary>
        <description>
            Log is kept in the configuration directory but the very
            last URL followed will be stored here to speed up startup
            so that session resume is implemented simply.
        </description>
    </key>

    </schema>
</schemalist>
